			 $(mod_dir)target.a)
ALL_MOD_STATIC_NAME	=$(ALL_MOD_AR_NAME)
ALL_MOD_STATIC_DIR	=$(foreach mod_name, $(ALL_MOD_AR_NAME), \
			 $(SRC)/$(mod_name)/)
ALL_MOD_STATIC		=$(foreach mod_dir, $(ALL_MOD_AR_DIR), \
			 $(mod_dir)target.a)

