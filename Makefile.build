ALL_MOD_NAME		=kernel tty lib tt
ALL_MOD_DIR		=$(foreach mod_name, $(ALL_MOD_NAME), \
			 $(SRC)/$(mod_name)/)
ALL_MOD			=$(foreach mod_dir, $(ALL_MOD_DIR), \
			 $(mod_dir)target.a)
ALL_MOD_AR_NAME		=kernel tty lib tt
ALL_MOD_AR_DIR		=$(foreach mod_name, $(ALL_MOD_AR_NAME), \
			 $(SRC)/$(mod_name)/)
